{
  "name": "tolisten",
  "version": "1.0.4",
  "description": "A free and open source music player to you!",
  "main": "build/index.js",
  "scripts": {
    "build": "rmdir /s /q build && tsc --project src/tsconfig.json",
    "build-dev": "npm run build && npx electron build/index.js",
    "dev": "npx electron build/index.js",
    "dist": "npm run build && rmdir /s /q dist && node builder/build.js"
  },
  "author": "DuckCoder1101",
  "license": "MIT",
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.1",
    "electron-updater": "^6.1.7",
    "node-downloader-helper": "^2.1.9",
    "uninstall": "^0.0.0"
  },
  "repository": {
    "url": "https://github.com/DuckCoder1101/ToListen",
    "type": "git"
  },
  "devDependencies": {
    "electron": "^27.1.3",
    "electron-builder": "^24.9.1"
  }
}
